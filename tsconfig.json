    "module": "ES2020",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./",
    "sourceMap": true
  },
  "include": [
    "script.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}